{
    "name": "mlk9/captcha-laravel",
    "description": "image captcha for Laravel 9+",
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Mohammad Maleki",
            "email": "malekii24@outlook.com"
        }
